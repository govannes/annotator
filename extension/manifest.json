{
  "manifest_version": 3,
  "name": "Annotator",
  "version": "0.1.0",
  "description": "Highlight and annotate any webpage with persistent anchors (Range, TextPosition, TextQuote).",
  "permissions": ["storage","sidePanel"],
  "side_panel":{"default_path":"sidepanel.html"},"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
